class Zoo {
  fun init() {
    this.aardvark = 1;
    this.baboon   = 1;
    this.cat      = 1;
    this.donkey   = 1;
    this.elephant = 1;
    this.fox      = 1;
  }
  fun ant()    { return this.aardvark; }
  fun banana() { return this.baboon; }
  fun tuna()   { return this.cat; }
  fun hay()    { return this.donkey; }
  fun grass()  { return this.elephant; }
  fun mouse()  { return this.fox; }
}

var zoo = Zoo();
var sum = 0;
var start = clock();
while (sum < 100000000) {
  sum = sum + zoo.ant()
            + zoo.banana()
            + zoo.tuna()
            + zoo.hay()
            + zoo.grass()
            + zoo.mouse();
}

print clock() - start;
print sum;